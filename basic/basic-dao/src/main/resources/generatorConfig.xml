<?xml version="1.0" encoding="UTF-8" ?>

    <!DOCTYPE generatorConfiguration 
      PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
      "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- java -jar mybatis-generator-core-1.3.2.jar -configfile ../src/generatorConfig.xml -->
<generatorConfiguration>
	<classPathEntry
		location="mysqljar????" />
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="???" userId="???"
			password="???">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

        <javaModelGenerator targetPackage="com.dada.{basic}.dao.model"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 需要按照库名变更文件夹 -->
        <sqlMapGenerator targetPackage="com.dada.{basic}.dao.mybatis.???"
			targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- 需要按照库名变更包名 -->
		<javaClientGenerator type="XMLMAPPER"
            targetPackage="com.dada.{basic}.dao.mapper.???" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- 需要按照表来变更 -->
		<table tableName="order" domainObjectName="OrderDO" />

	</context>

</generatorConfiguration>
